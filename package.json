{
  "name": "kio-ng2-cli",
  "version": "0.0.6",
  "description": "cli tool for kio ng2 projects",
  "main": "index.js",
  "bin": {
    "kio-ng2": "./bin/exec.js"
  },
  "scripts": {
    "prebuild": "rm -rf ./release",
    "prebuild:production": "rm -rf ./release; npm run doc",
    "build": "NODE_ENV=production tsc",
    "build:production": "NODE_ENV=production tsc",
    "doc": "typedoc --out documentation",
    "preversion": "npm run build:production",
    "test": "NODE_ENV=debug ts-mocha ./src/project.spec.ts",
    "test:watch": "nodemon -w ./src -e ts --exec \"npm run test\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "ceylon": "^0.9.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "ts-mocha": "^1.0.3",
    "tslint": "^5.0.0",
    "typedoc": "^0.5.10",
    "typescript": "^2.2.2"
  },
  "dependencies": {
    "@types/ejs": "^2.3.33",
    "@types/ember": "^2.7.37",
    "@types/node": "^7.0.12",
    "@types/shelljs": "^0.7.0",
    "@types/yargs": "^6.6.0",
    "chalk": "^1.1.3",
    "ejs": "^2.5.6",
    "ember-source": "^2.12.1",
    "kio-ng2": "github:afkmatze/kio-ng2",
    "kio-ng2-component-routing": "github:afkmatze/kio-ng2-component-routing",
    "rxjs": "^5.3.0",
    "shelljs": "^0.7.7",
    "yargs": "^7.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afkmatze/kio-ng2-cli.git"
  },
  "bugs": {
    "url": "https://github.com/afkmatze/kio-ng2-cli/issues"
  },
  "homepage": "https://github.com/afkmatze/kio-ng2-cli#readme"
}
